    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
      <if test="entrustmentNumber != null">
        #{entrustmentNumber,jdbcType=DOUBLE},
